//create background
Ti.UI.setBackgroundColor("#fff");

//create main Window
var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#000",
});

//create top view to display changing text
var myfirstView = Ti.UI.createView({
	backgroundColor: "fff",
	borderRadius: 5,
	borderWidth: 1,
	width: 35,
	height: 35
	
});

//call views
mainWindow.add(myfirstView);
mainWindow.open();

